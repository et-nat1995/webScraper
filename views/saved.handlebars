<div class="row">
    <div class="col md">
        {{#each dbArt}}
        <div class="card mb-2 mr-2 ml-2">
            <div class="card-header">
                <h3 style="width: 80%;"><a class="article-link" target="_blank"  href="{{link}}">{{title}}</a>
                    <a class="btn btn-danger delete" data-id="{{_id}}">Delete</a>
                    <a class="btn btn-warning note" data-id="{{_id}}">Note</a></h3>
            </div>
            <div class="card-body">Need to add card summary</div>
        </div>
        {{/each}}
    </div>
</div>
